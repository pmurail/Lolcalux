{% extends 'base.html.twig' %}

{% block title %}Hello HomeController!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">
  
  {{form_start(monForm)}}
        {{form_row(monForm.nom)}}
        {{form_row(monForm.prenom)}}
        {{form_row(monForm.tel)}}
        {{form_row(monForm.email)}}
        {{form_row(monForm.rue)}}
        {{form_row(monForm.cp)}}
        {{form_row(monForm.ville)}}
        {{form_row(monForm.login)}}
        {{form_row(monForm.mdp)}}
        <input type="submit"></input>
    {{form_end(monForm)}}

</div>
{% endblock %}
